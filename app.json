{
  "expo": {
    "name": "PAYO",
    "slug": "payo-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/monochrome-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/monochrome-icon.png",
      "resizeMode": "",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "icon": "./assets/images/filled-icon-final.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/filled-icon-final.png",
        "monochromeImage": "./assets/images/adaptive-icon-final.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.riwajprasai123.payofrontend"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow PAYO to access your camera",
          "microphonePermission": "Allow PAYO to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      "expo-router",

      [
        "expo-splash-screen",
        {
          "backgroundColor": "#000000",
          "image": "./assets/images/monochrome-icon.png",
          "dark": {
            "image": "./assets/images/monochrome-icon.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ec0da39e-7bc9-48a5-913d-cfd6869acf9d"
      }
    }
  }
}
